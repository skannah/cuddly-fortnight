pragma solidity ^0.8.9;

contract Crowdfunding {
    struct Campaign {
         address ownder;
        string title;
        string description;
        uint256 target;
        uint256 deadline;
        uint256 amountCollected;
        string image;
        address[] donators;
        uint256[] donations;
    }
       

}
